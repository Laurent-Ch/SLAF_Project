
<%- if devise_mapping.omniauthable? %>
<%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "#{OmniAuth::Utils.camelize(provider).gsub("Oauth2", "")}", omniauth_authorize_path(resource_name, provider), method: :post, class: "button__normal button__form loginbtn--#{provider}"  %>
    <% end %>
<% end %>




